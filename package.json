{
  "name": "pdfdataextract",
  "version": "4.0.0",
  "description": "Extract data from a pdf with pure javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" --cache",
    "lint:fix": "eslint \"src/**/*.ts\" --cache --fix",
    "build": "tsc",
    "test": "vitest",
    "prepare": "npm run build",
    "build:doc": "typedoc src/index.ts"
  },
  "keywords": [
    "pdf",
    "pdf.js",
    "pdfjs",
    "parse",
    "parser",
    "json",
    "text",
    "data",
    "crawler",
    "extract",
    "extractor",
    "ocr",
    "converter",
    "image"
  ],
  "dependencies": {
    "pdfjs-dist": "5.0.375"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@types/node": "^22.13.14",
    "@types/opentype.js": "^1.3.8",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "canvas": "^3.1.0",
    "eslint": "^9.23.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "globals": "^16.0.0",
    "pureimage": "^0.4.18",
    "skia-canvas": "^2.0.2",
    "tesseract.js": "^6.0.0",
    "typedoc": "^0.28.1",
    "typedoc-github-wiki-theme": "^2.1.0",
    "typedoc-plugin-markdown": "^4.6.0",
    "typescript": "5.8.2",
    "vitest": "^3.0.9"
  },
  "homepage": "https://github.com/lublak/pdfdataextract#readme",
  "bugs": {
    "url": "https://github.com/lublak/pdfdataextract/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lublak/pdfdataextract.git"
  },
